  <div class="container">
    <div class="jumbotron">
      <h1 class="jumboheader">New York Times Scraper</h1>
      <p class="lead">Get and save the latest news from New York Times.</p>
      <p>
        <a class="btn btn-lg" href="/scrape" role="button">Search New Articles</a>
      </p>
      <p>
        <a class="btn btn-lg" href="/saved" role="button">Go to Saved Articles</a>
      </p>
    </div>

    <div class="row">
      <div class="col-sm-12 scraped-articles">
        {{#each result}}
        <div class="card news-section">
          <a href="{{this.link}}">
            <h4 class="card-header" id="title">{{this.title}}</h4>
          </a>
          <div class="card-block">
            <a id="link" href="{{this.link}}">
              <p class="card-text" id="snippet">{{this.articleSnippet}}</p>
            </a>
            <br>
            <a class="save btn btn-primary">Save</a>
          </div>
        </div>
        <br>
        <br> {{/each}}
      </div>
    </div>
    
  </div>